    "redis": "4.0.3",
    "redis-mock": "0.56.3",
    "sequelize": "^6.16.1",
    "sequelize-cli": "6.4.1",
    "sqlite3": "5.0.2",
    "whatwg-url": "11.0.0"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "braces": "3.0.2",
    "chai": "4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
