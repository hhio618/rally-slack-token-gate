    "url": "https://github.com/hhio618/rally-slack-token-gate/issues"
  },
  "homepage": "https://github.com/hhio618/rally-slack-token-gate#readme",
  "dependencies": {
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.5",
    "@node-redis/client": "1.0.3",
    "@slack/bolt": "^3.9.0",
    "@slack/client": "^5.0.2",
    "auto-parse": "1.8.0",
    "axios": "^0.25.0",
    "child-process-promise": "2.2.1",
    "crypto": "1.0.1",
