{
  "name": "rally-slack-token-gate",
  "version": "1.0.0",
  "description": "Token Gating Slack Channels",
  "main": "src/app.js",
  "scripts": {
    "db:create-migration": "node ./scripts/db/createMigration",
    "db:migrate": "node ./scripts/db/migrate",
    "db:rollback": "node ./scripts/db/rollback",
    "sqlite:clear": "rm -rf test.sqlite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretest": "NODE_ENV=test npm run db:migrate",
    "test": "NODE_ENV=test mocha 'test/**/*.js' -r dotenv/config  --recursive --timeout 60000 --exit",
    "test:coverage": "NODE_ENV=test nyc mocha",
    "test:report": "nyc report --reporter=text-summary",
    "start": "node ./src/app.js"
  },
  "repository": {
    "type": "git",
